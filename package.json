{
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --no-notify ./src/index.ts",
    "build": "tsc --build tsconfig.json",
    "lint": "tslint --project .",
    "db:migrate-up": "migrate up --migrations-dir ./db/migrations --store ./db/pg-state-store.js",
    "db:migrate-down": "migrate down --migrations-dir ./db/migrations --store ./db/pg-state-store.js",
    "db:migrate-create": "migrate create --migrations-dir ./db/migrations"
  },
  "engines": {
    "node": "14.17.x",
    "yarn": "1.22.x"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/gapi": "^0.0.39",
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^13.9.2",
    "@types/pg": "^7.14.3",
    "@types/query-string": "^6.3.0",
    "@types/uuid": "^7.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fpx": "^0.7.2",
    "googleapis": "^48.0.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^8.0.8",
    "migrate": "^1.6.2",
    "pg": "^7.18.2",
    "pg-native": "^3.0.0",
    "query-string": "^6.11.1",
    "uuid": "^7.0.2",
    "xhttp": "^0.12.0"
  }
}
